{
  "expo": {
    "name": "Autism Saathi",
    "slug": "autism-support-parents",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "autism-support-parents",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "description": "Supportive and educational app for Nepali families with children on the autism spectrum.",

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.binayapaudel.autismsupportparents",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],

    "runtimeVersion": {
      "policy": "sdkVersion"
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "50d34e6b-dadc-499b-bb52-8c2439b4d997"
      }
    }
  }
}
